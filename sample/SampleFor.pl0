var i, count;
procedure proc;

begin
    count := 0;
        for i := 1 to 9 do
            begin
                count := count + i;
            end;
    write(count);
end;

begin
    count := 0;
        for i := 10 downto 1 do
            begin
                count := count + i;
            end;
    write(count);
    call proc;
end.
